<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="autoRotateTextView">
        <attr name="text_size"/>
        <attr name="text_color"/>
        <attr name="gravity"/>
    </declare-styleable>

    <declare-styleable name="RingView">
        <attr name="innerCircleColor" format="color" />
        <attr name="innerCircleRadius" format="dimension" />
        <attr name="ringWidth" format="dimension" />
        <attr name="ringBaseColor" format="color" />
        <attr name="ringProgressColor" format="color" />
        <attr name="autoSize" format="boolean" />
        <attr name="progressTextColor" format="color" />
        <attr name="progressTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RoundShadowConstraintLayout">
        <attr name="round_fill_in_color" format="color" />
        <attr name="round_corner_radius" format="dimension" />
        <attr name="round_shadow_color" format="color" />
        <attr name="round_blur_radius" format="dimension" />
        <attr name="round_stroke_width" format="dimension" />
    </declare-styleable>
</resources>